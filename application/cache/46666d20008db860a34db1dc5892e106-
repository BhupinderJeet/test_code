<script>
    $(document).ready(function(){
        $("#aged_from").change(function(){
            var from = parseInt($("#aged_from").val());
            var to = parseInt($("#aged_to").val());
            if (to < from) {
                var to = $("#aged_to").val(from);
            }
        });
        $("#aged_to").change(function(){
            var from = parseInt($("#aged_from").val());
            var to = parseInt($("#aged_to").val());
            if (to < from) {
                var to = $("#aged_to").val(from);
            }
        });
    });

     $(".s_religion").change(function(){
        var religion_id = $(".s_religion").val();
        if (religion_id == "") {
            $(".s_caste").html("<option value=''>Choose A Religion First</option>");
            $(".s_sub_caste").html("<option value=''>Choose A Caste First</option>");
        } else {
            $.ajax({
                url: "http://dev.orishta.com/home/get_dropdown_by_id_caste/caste/religion_id/"+religion_id, // form action url
                type: 'POST', // form submit method get/post
                dataType: 'html', // request type html/json/xml
                cache       : false,
                contentType : false,
                processData : false,
                success: function(data) {
                    $(".s_caste").html(data);
                    $(".s_sub_caste").html("<option value=''>Choose A Caste First</option>");
                },
                error: function(e) {
                    console.log(e)
                }
            });
        }
    });

    $(".s_caste").change(function(){
        var caste_id = $(".s_caste").val();
        if (caste_id == "") {
            $(".s_sub_caste").html("<option value=''>Choose A Caste First</option>");
        } else {
            $.ajax({
                url: "http://dev.orishta.com/home/get_dropdown_by_id_sub_caste/sub_caste/caste_id/"+caste_id, // form action url
                type: 'POST', // form submit method get/post
                dataType: 'html', // request type html/json/xml
                cache       : false,
                contentType : false,
                processData : false,
                success: function (data) {
                    if(data == false ){
                        $(".s_sub_caste").html("<option value=''>None</option>");
                    } else {
                        $(".s_sub_caste").html(data);
                    };
               },
                error: function(e) {
                    console.log(e)
                }
            });
        }
    });
</script>                        <footer id="footer" class="footer">
